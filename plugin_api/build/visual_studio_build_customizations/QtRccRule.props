<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemDefinitionGroup>
	<QtRCC>
	  <OutputFileName>$(QtRccOutput)\res_%(Filename).cpp</OutputFileName>
      <CommandLineTemplate>"$(QsfDir)build\visual_studio_scripts\qt_rcc.bat" $(QtRccExecutable) %(FullPath) $(QtRccOutput) %(Filename)</CommandLineTemplate>
      <ExecutionDescription>Compiling %(Filename)%(Extension) using Qt RCC</ExecutionDescription>
    </QtRCC>
  </ItemDefinitionGroup>
</Project>
