<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemDefinitionGroup>
	<QtMOC>
	  <OutputFileName>$(QtMocOutput)\moc_%(Filename).cpp</OutputFileName>
      <CommandLineTemplate>"$(QsfDir)build\visual_studio_scripts\qt_moc.bat" $(QtMocExecutable) %(FullPath) $(QtMocOutput) %(Filename) $(ProjectName)</CommandLineTemplate>
      <ExecutionDescription>Compiling %(Filename)%(Extension) using Qt MOC</ExecutionDescription>
    </QtMOC>
  </ItemDefinitionGroup>
</Project>
